{
    "files.exclude": {
        "**/*.acebase/": true,
        "db/": true,
        "**/*.tsv": true,
        "**/*.backup": true,
        "garbage/": true,
        "ignore/gazerCount.json": true,
        "node_modules/": true,
        "**/*.v8": true,
        "**/*.csv": true
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.format.enable": true,
    "eslint.options": {
        "cache": true
    },
    "workbench.colorCustomizations": {
        "activityBar.background": "#620302",
        "titleBar.activeBackground": "#890403",
        "titleBar.activeForeground": "#FFFBFB",
        "statusBar.background": "#890403",
        "statusBar.foreground": "#FFFBFB"
    }
}
